if(Git_FOUND)
  execute_process(
    COMMAND ${GIT_EXECUTABLE} \-\-no\-pager log \-n 1 HEAD \(dq\-\-pretty=format:%h %s\(dq
    OUTPUT_VARIABLE output
    RESULT_VARIABLE result
    ERROR_QUIET
    OUTPUT_STRIP_TRAILING_WHITESPACE
