.INDENT 3.5
.sp
.EX
find_package(RTI)

if(RTI_FOUND AND NOT TARGET RTI::RTI)
  add_library(RTI::RTI INTERFACE IMPORTED)
  set_target_properties(
    RTI::RTI
    PROPERTIES
      INTERFACE_INCLUDE_DIRECTORIES \(dq${RTI_INCLUDE_DIR}\(dq
      INTERFACE_LINK_LIBRARIES \(dq${RTI_LIBRARIES}\(dq
      INTERFACE_COMPILE_DEFINITIONS \(dq${RTI_DEFINITIONS}\(dq
  )
